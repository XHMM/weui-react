{
  "name": "@xhmm/weui-react",
  "version": "0.1.0",
  "description": "weui for react, made mainly for personal use",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "keywords": [
    "react",
    "weui"
  ],
  "scripts": {
    "lib:watch": "rollup -c --watch",
    "lib:build": "rimraf ./build && rollup -c && tsc",
    "lib:eslint": "eslint ./src --ext .ts --ext .tsx --ignore-path ./.gitignore --ignore-path ./.eslintignore --fix-dry-run",
    "prepublishOnly": "npm run lib:build",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "files": [
    "build"
  ],
  "private": false,
  "author": "xhmm",
  "license": "MIT",
  "repository": "https://github.com/XHMM/weui-react",
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@material-ui/core": "^4.9.3",
    "@material-ui/icons": "^4.9.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@types/classnames": "^2.2.9",
    "@types/react": "16.9.21",
    "@types/react-dom": "16.9.5",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cross-env": "7.0.0",
    "docz": "^2.2.0",
    "eslint": "6.8.0",
    "eslint-config-xhmm": "0.1.9",
    "prettier": "1.19.1",
    "prettier-config-xhmm": "0.0.6",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-css-only": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "typescript": "3.8.2"
  }
}
