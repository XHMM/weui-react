{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["dom", "dom.iterable","es2015", "es2016", "es2018"],
    "jsx": "react",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["../site"],
  "exclude": ["node_modules"]
}
